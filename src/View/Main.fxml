<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="684.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MainController">
   <children>
      <MenuBar layoutX="-1.0" layoutY="1.0" prefHeight="25.0" prefWidth="689.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" text="Save" />
              <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" onAction="#onMainMenuItemSelected" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem id="mainMenuItemAbout" fx:id="mainMenuItemAbout" mnemonicParsing="false" onAction="#onMenuItemSelected" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Pane layoutX="14.0" layoutY="32.0" prefHeight="359.0" prefWidth="134.0">
         <children>
            <Button fx:id="addExamBtn" layoutX="17.0" layoutY="14.0" mnemonicParsing="false" onAction="#onAddExamBtn" prefHeight="25.0" prefWidth="100.0" text="Add" />
            <Button fx:id="editExamBtn" layoutX="17.0" layoutY="48.0" mnemonicParsing="false" onAction="#onEditExamBtn" prefHeight="25.0" prefWidth="100.0" text="Edit" />
            <Button fx:id="removeExamBtn" layoutX="17.0" layoutY="80.0" mnemonicParsing="false" onAction="#onRemoveExamBtn" prefHeight="25.0" prefWidth="100.0" text="Remove" />
            <Button fx:id="exportBtn" layoutX="9.0" layoutY="320.0" mnemonicParsing="false" onAction="#onExportBtn" prefHeight="25.0" prefWidth="116.0" text="Export" />
            <Button fx:id="manageDataBtn" layoutX="5.0" layoutY="280.0" mnemonicParsing="false" onAction="#onManageDataBtn" prefHeight="25.0" prefWidth="122.0" text="Manage Data" />
         </children>
      </Pane>
      <Pane layoutX="155.0" layoutY="32.0" prefHeight="359.0" prefWidth="530.0">
         <children>
            <TableView fx:id="tableView" layoutX="1.0" layoutY="2.0" prefHeight="359.0" prefWidth="526.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Mon" />
                <TableColumn prefWidth="75.0" text="Tue" />
                  <TableColumn prefWidth="75.0" text="Wed" />
                  <TableColumn prefWidth="75.0" text="Thu" />
                  <TableColumn prefWidth="75.0" text="Fri" />
                  <TableColumn prefWidth="75.0" text="Sat" />
                  <TableColumn prefWidth="75.0" text="Sun" />
              </columns>
            </TableView>
         </children></Pane>
   </children>
</AnchorPane>
